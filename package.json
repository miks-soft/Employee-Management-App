    "expo-linear-gradient": "~12.1.2",
    "expo-location": "~15.1.1",
    "expo-navigation-bar": "~2.1.1",
    "expo-sensors": "11.2.0",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-task-manager": "~11.1.1",
    "geolib": "^3.3.3",
