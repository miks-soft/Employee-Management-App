{
  "name": "Employee Management App",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:se2": "npx react-native run-ios --simulator='iPhone SE (2nd generation)'",
    "init:dev": "cp ./internals/.env.development ./.env",
    "pods": "cd ios && pod install && cd ..",
    "pods-fabric": "cd ios && RCT_NEW_ARCH_ENABLED=1 pod install && cd ..",
    "check-types": "tsc --noEmit",
    "find-deadcode": "ts-prune | grep -v -E '(used in module)'",
    "process-api-types": "npx ts-node-esm  ./scripts/process-types-from-json.ts",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint ./src --fix --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest",
    "plop": "npx plop",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@notifee/react-native": "^7.6.1",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-camera-roll/camera-roll": "^5.3.1",
